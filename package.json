{
	"name": "@imhuso/unocss-preset-daisy",
	"version": "8.2.0",
	"description": "UnoCSS preset for daisyUI",
	"scripts": {
		"build": "tsc -p tsconfig.build.json && vite build demo",
		"prepare": "tsc -p tsconfig.build.json",
		"preview": "vite preview demo --host",
		"start": "vite demo --host",
		"release": "bumpp package.json --commit --push --tag && pnpm publish --access public",
		"test": "vitest watch",
		"up": "taze major -w"
	},
	"repository": "imhuso/unocss-preset-daisy",
	"license": "MIT",
	"author": "imhuso <imhuso@outlook.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./distribution/index.d.ts",
			"import": "./distribution/index.js",
			"require": "./distribution/index.js"
		}
	},
	"main": "./distribution/index.js",
	"module": "./distribution/index.js",
	"types": "./distribution/index.d.ts",
	"files": [
		"distribution"
	],
	"dependencies": {
		"@types/css-tree": "^2.3.9",
		"autoprefixer": "^10.4.20",
		"camelcase": "^8.0.0",
		"css-selector-tokenizer": "^0.8.0",
		"css-tree": "^3.0.1",
		"css-what": "^6.1.0",
		"postcss": "^8.4.49",
		"postcss-js": "^4.0.1"
	},
	"peerDependencies": {
		"daisyui": "^4.7.3",
		"unocss": ">=0.64.1"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^3.11.2",
		"@sindresorhus/tsconfig": "^7.0.0",
		"@types/css-selector-tokenizer": "^0.7.4",
		"@types/node": "^20.17.9",
		"@types/postcss-js": "^4.0.4",
		"@unocss/preset-uno": "^0.64.1",
		"@unocss/reset": "^0.64.1",
		"@unocss/vite": "^0.64.1",
		"@vitest/coverage-v8": "^2.1.6",
		"bumpp": "^9.8.1",
		"eslint": "^9.16.0",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vitest": "^2.1.6"
	},
	"keywords": [
		"unocss",
		"unocss-preset",
		"daisyui"
	],
	"engines": {
		"node": ">=18.0.0"
	}
}
